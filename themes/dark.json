{
  "name": "Lexifry Dark",
  "colors": {
    "editor.background": "#16161d",
    "editor.foreground": "#FFFAF0",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // default
    "variable": "#FFFAF0",
    "operator": "#FFFAF0",
    // keywords
    "keyword": "#fc0fc0",
    "operator.controlFlow": "#fc0fc0",
    "selfTypeKeyword:rust": "#fd5185",
    // values
    "boolean": "#FDD50D",
    "formatSpecifier": "#FDD50D",
    "enumMember:rust": "#CEE686",
    // types
    "builtinType": "#9FF6FE",
    "typeAlias": "#9FF6FE",
    "enum": "#9FF6FE",
    "struct": "#9FF6FE",
    "interface": "#95BFC3",
    "derive": "#95BFC3",
    "typeParameter": "#A6BEBF",
    // identifiers
    "function": "#9683EC",
    "method": "#9683EC",
    "namespace": "#CFF8F7",
    "property": "#FFED9C",
    "tomlArrayKey": "#FFED9C",
    "tomlTableKey": "#FFED9C",
    // macros/etc.
    "macro": "#C4BEE4",
    "decorator": "#C4BEE4",
    "deriveHelper:rust": "#C4BEE4",
    "attributeBracket:rust": "#C4BEE4",
    "operator.mutable:rust": "#C4BEE4"
  },
  "tokenColors": [
    {
      "name": "comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#61495C"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "keyword.operator.question.rust",
        "keyword.operator.borrow.and.rust",
        "keyword.operator.dereference",
        "punctuation.definition.keyword.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.css punctuation"
      ],
      "settings": {
        "foreground": "#fc0fc0"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "keyword.operator",
        "punctuation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#CE83DF"
      }
    },
    {
      "name": "punctuation (value-related)",
      "scope": [
        "keyword.operator.access.dot.rust",
        "punctuation.accessor",
        "punctuation.definition.array",
        "meta.brace.square.ts",
        "source.css keyword.other.unit",
        "constant.other.color.rgb-value.hex.css punctuation",
        "source.tsx punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#A9C0C2"
      }
    },
    {
      "name": "punctuation (type-related)",
      "scope": [
        "punctuation.brackets.angle.rust",
        "keyword.operator.namespace.rust",
        "meta.brace.angle.ts"
      ],
      "settings": {
        "foreground": "#A6BEBF"
      }
    },
    {
      "name": "punctuation (function-related)",
      "scope": [
        "punctuation.brackets.round.rust",
        "meta.brace.round.ts",
        "meta.brace.round.tsx",
        "punctuation.definition.parameters.begin.tsx",
        "punctuation.definition.parameters.end.tsx",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx"
      ],
      "settings": {
        "foreground": "#A19CB8"
      }
    },
    {
      "name": "punctuation (operators)",
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.math",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FFFAF0"
      }
    },
    {
      "name": "punctuation (overloadable operators)",
      "scope": [
        "keyword.operator.comparison.rust",
        "keyword.operator.dereference.rust",
        "keyword.operator.math.rust",
        "keyword.operator.logical.rust"
      ],
      "settings": {
        "foreground": "#FFBAE9"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string",
        "punctuation.definition.string",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation"
      ],
      "settings": {
        "foreground": "#FDA43A"
      }
    },
    {
      "name": "string escapes",
      "scope": [
        "punctuation.definition.interpolation",
        "meta.interpolation",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FDD50D"
      }
    },
    // types
    {
      "name": "Types",
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#9FF6FE"
      }
    },
    // values
    {
      "name": "keyword constants",
      "scope": [
        "constant.language",
        "source.css support.constant"
      ],
      "settings": {
        "foreground": "#FDD50D"
      }
    },
    {
      "name": "numbers",
      "scope": [
        "constant.numeric",
        "constant.other.color.rgb-value.hex.css"
      ],
      "settings": {
        "foreground": "#FDD50D"
      }
    },
    // HTML / etc.
    {
      "name": "HTML tag names",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.tsx",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#9683EC"
      }
    },
    {
      "name": "HTML attribute names",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#FFED9C"
      }
    },
    {
      "name": "HTML doctype",
      "scope": [
        "meta.tag.metadata.doctype.html entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#fc0fc0"
      }
    },
    // CSS
    {
      "name": "CSS functions",
      "scope": [
        "support.function.misc.css"
      ],
      "settings": {
        "foreground": "#9683EC"
      }
    },
    // JSON
    {
      "name": "JSON property names",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.json punctuation"
      ],
      "settings": {
        "foreground": "#fc0fc0"
      }
    },
    {
      "name": "JSON string values",
      "scope": [
        "source.json string",
        "source.json string punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#FFFAF0"
      }
    },
    // YAML
    {
      "name": "YAML property names",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#fc0fc0"
      }
    },
    {
      "name": "YAML unquoted strings",
      "scope": [
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#FFFAF0"
      }
    },
    // TOML
    {
      "name": "TOML table names",
      "scope": [
        "support.type.property-name.table.toml"
      ],
      "settings": {
        "foreground": "#fc0fc0"
      }
    },
    {
      "name": "TOML property names",
      "scope": [
        "support.type.property-name.toml"
      ],
      "settings": {
        "foreground": "#FFED9C"
      }
    }
  ]
}